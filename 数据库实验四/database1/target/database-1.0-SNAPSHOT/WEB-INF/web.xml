<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">
    <servlet>
        <servlet-name>LoginServlet</servlet-name>
        <servlet-class>com.example.database.LoginServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>LoginServlet</servlet-name>
        <url-pattern>/LoginServlet</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>RegisterServlet</servlet-name>
        <servlet-class>com.example.database.RegisterServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>RegisterServlet</servlet-name>
        <url-pattern>/RegisterServlet</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>RecordPageServlet</servlet-name>
        <servlet-class>com.example.database.RecordPageServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>RecordPageServlet</servlet-name>
        <url-pattern>/RecordPageServlet</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>FeedPageServlet</servlet-name>
        <servlet-class>com.example.database.FeedPageServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>FeedPageServlet</servlet-name>
        <url-pattern>/FeedPageServlet</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>MainPageServlet</servlet-name>
        <servlet-class>com.example.database.MainPageServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>MainPageServlet</servlet-name>
        <url-pattern>/MainPageServlet</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>CheckInServlet</servlet-name>
        <servlet-class>com.example.database.CheckInServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>CheckInServlet</servlet-name>
        <url-pattern>/CheckInServlet</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>ClockInServlet</servlet-name>
        <servlet-class>com.example.database.ClockInServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ClockInServlet</servlet-name>
        <url-pattern>/ClockInServlet</url-pattern>
    </servlet-mapping>

    <welcome-file-list>
        <welcome-file>login.jsp</welcome-file>
    </welcome-file-list>
</web-app>